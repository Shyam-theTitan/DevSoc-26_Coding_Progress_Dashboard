    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.0",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=16.0.0"
